{
  "name": "eo-locale",
  "version": "7.3.0",
  "description": "Easy way to internationalize your React application",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">= 10"
  },
  "sideEffects": false,
  "scripts": {
    "prebuild": "npm run lint && npm run test ",
    "build": "rm -rf dist && rollup --config ./rollup.config.js",
    "postbuild": "copyup \"src/**/*.flow\" dist && size-limit",
    "lint": "tslint \"src/**/*.ts\" \"src/**/*.tsx\" --fix --config ./config/tslint.json",
    "preversion": "flow && npm run build",
    "test": "jest --detectOpenHandles --config ./config/jest.config.json --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibitcy/eo-locale.git"
  },
  "keywords": [
    "internationalization",
    "intl",
    "messageformat",
    "plural",
    "icu",
    "react",
    "tiny",
    "i18n"
  ],
  "author": {
    "name": "Maksim Sharipov",
    "url": "https://github.com/pret-a-porter"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ibitcy/eo-locale/issues"
  },
  "homepage": "https://ibitcy.github.io/eo-locale/",
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "900B",
      "webpack": false
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.0.2",
    "@types/enzyme": "^3.1.11",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^25.1.3",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.9",
    "copyfiles": "^2.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "flow-bin": "^0.122.0",
    "husky": "^4.0.10",
    "intl": "^1.2.5",
    "intl-pluralrules": "^1.1.1",
    "jest": "^25.1.0",
    "jest-haste-map": "^25.1.0",
    "jest-resolve": "^25.1.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rollup": "^2.3.4",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "size-limit": "^4.0.2",
    "ts-jest": "^25.2.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^3.0.0"
  },
  "dependencies": {
    "@eo-locale/core": "^1.2.1"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
