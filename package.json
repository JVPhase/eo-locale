{
  "name": "eo-locale",
  "version": "5.0.8",
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && rollup --config ./rollup.config.js && npm run check-size --scripts-prepend-node-path",
