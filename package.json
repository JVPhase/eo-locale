{
  "name": "eo-locale",
  "version": "7.0.6",
  "description": "Easy way to internationalize your React application",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">= 8"
  },
  "sideEffects": false,
  "scripts": {
    "prebuild": "npm run lint && npm run test ",
    "build": "rm -rf dist && rollup --config ./rollup.config.js",
    "postbuild": "copyup \"src/**/*.flow\" dist && size-limit",
    "docz:build": "docz build",
    "docz:dev": "docz dev",
    "lint": "tslint \"src/**/*.ts\" \"src/**/*.tsx\" --fix --config ./config/tslint.json",
    "preversion": "flow && npm run build",
    "test": "jest --detectOpenHandles --config ./config/jest.config.json --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibitcy/eo-locale.git"
  },
  "keywords": [
    "internationalization",
    "intl",
    "messageformat",
    "plural",
    "icu",
    "react",
    "tiny",
    "i18n"
  ],
  "author": {
    "name": "Maksim Sharipov",
    "url": "https://github.com/pret-a-porter"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ibitcy/eo-locale/issues"
  },
  "homepage": "https://ibitcy.github.io/eo-locale/",
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "800B",
      "webpack": false
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^2.1.6",
    "@types/enzyme": "^3.1.11",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^24.0.0",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.9",
    "copyfiles": "^2.1.1",
    "docz": "^1.0.0",
    "docz-theme-default": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "flow-bin": "^0.114.0",
    "husky": "^3.0.0",
    "intl": "^1.2.5",
    "intl-pluralrules": "^1.1.1",
    "jest": "^24.0.0",
    "jest-haste-map": "^24.5.0",
    "jest-resolve": "^24.5.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rollup": "^1.6.0",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-jest": "^24.0.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^3.0.0"
  },
  "dependencies": {
    "@eo-locale/core": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
