    "test": "jest --detectOpenHandles --config ./config/jest.config.json --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibitcy/eo-locale.git"
  },
  "keywords": [
    "internationalization",
    "intl",
    "messageformat",
    "plural",
    "icu",
    "react",
    "tiny",
    "i18n"
  ],
  "author": {
    "name": "Maksim Sharipov",
    "url": "https://github.com/pret-a-porter"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ibitcy/eo-locale/issues"
  },
  "homepage": "https://github.com/ibitcy/eo-locale#readme",
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "2kb"
    }
  ],
  "devDependencies": {
    "@types/enzyme": "^3.1.11",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/intl": "^1.2.0",
    "@types/intl-locales-supported": "^1.0.0",
    "@types/jest": "^24.0.0",
    "@types/node": "^11.0.0",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.9",
    "@types/webpack-env": "^1.13.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^1.1.3",
    "jest": "^24.0.0",
    "jest-haste-map": "^24.5.0",
    "jest-resolve": "^24.5.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rollup": "^1.6.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.20.1",
    "size-limit": "^0.21.1",
    "ts-jest": "^24.0.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^3.0.0"
  },
  "dependencies": {
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
