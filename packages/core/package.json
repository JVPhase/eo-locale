{
  "name": "@eo-locale/core",
  "version": "1.5.1",
  "description": "Easy way to localize your JavaScript application",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "type": "module",
  "exports": {
    "import": "./lib/index.js",
    "require": "./lib/index.cjs"
  },
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "build": "rollup --config config/rollup.config.js",
    "postbuild": "copyup \"src/**/*.flow\" lib && size-limit",
    "prepare": "npm run test && npm run build",
    "test": "jest --detectOpenHandles --config config/jest.config.json --no-cache"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "sideEffects": false,
  "size-limit": [
    {
      "path": "lib/index.cjs",
      "limit": "1.3kb",
      "webpack": false
    }
  ],
  "keywords": [
    "internationalization",
    "intl",
    "messageformat",
    "plural",
    "icu",
    "tiny",
    "i18n"
  ],
  "author": {
    "name": "Maksim Sharipov",
    "url": "https://github.com/pret-a-porter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibitcy/eo-locale.git"
  },
  "bugs": {
    "url": "https://github.com/ibitcy/eo-locale/issues"
  },
  "license": "ISC",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.6.0",
    "@types/jest": "^26.0.13",
    "copyfiles": "^2.3.0",
    "flow-bin": "^0.130.0",
    "jest": "^26.4.2",
    "rollup": "^2.26.11",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "size-limit": "^4.6.0",
    "ts-jest": "^26.3.0",
    "typescript": "^4.6.4"
  },
  "gitHead": "85f16888e69bd8962e3e20ad92065a6ef5afcb22"
}
