{
  "name": "@eo-locale/core",
  "version": "1.3.0",
  "description": "",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "build": "rollup --config ./rollup.config.js",
    "postbuild": "copyup \"src/**/*.flow\" lib && size-limit",
    "prepare": "flow && npm run test && npm run build",
    "test": "jest --detectOpenHandles --config ./config/jest.config.json --no-cache"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "1.2kb",
      "webpack": false
    }
  ],
  "keywords": [
    "internationalization",
    "intl",
    "messageformat",
    "plural",
    "icu",
    "tiny",
    "i18n"
  ],
  "author": {
    "name": "Maksim Sharipov",
    "url": "https://github.com/pret-a-porter"
  },
  "license": "ISC",
  "dependencies": {
    "dlv": "^1.1.3"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.0.2",
    "@types/dlv": "^1.1.2"
  }
}
