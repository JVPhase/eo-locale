{
  "name": "@eo-locale/preact",
  "version": "1.4.8",
  "description": "Easy way to localize your Preact application",
  "homepage": "https://eo-locale.netlify.app",
  "main": "lib/index.js",
  "type": "module",
  "exports": {
    "import": "./lib/index.es.js",
    "require": "./lib/index.js"
  },
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">= 12"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup --config config/rollup.config.js && npm run check-size --scripts-prepend-node-path",
    "check-size": "size-limit",
    "preversion": "npm run test && npm run build"
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "800B",
      "webpack": false
    }
  ],
  "keywords": [],
  "author": {
    "name": "Maksim Sharipov",
    "url": "https://github.com/pret-a-porter"
  },
  "license": "ISC",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.6.0",
    "preact": "^10.1.1",
    "rollup": "^2.26.11",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "size-limit": "^4.6.0",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "preact": "^10.1.1"
  },
  "dependencies": {
    "@eo-locale/core": "^1.4.8"
  }
}
