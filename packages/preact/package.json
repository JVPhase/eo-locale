{
  "name": "@eo-locale/preact",
  "version": "1.6.0-beta.0",
  "description": "Easy way to localize your Preact application",
  "homepage": "https://eo-locale.netlify.app",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "type": "module",
  "exports": {
    "import": "./lib/index.js",
    "require": "./lib/index.cjs"
  },
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">= 12"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup --config config/rollup.config.js",
    "postbuild": "size-limit",
    "preversion": "npm run build"
  },
  "size-limit": [
    {
      "path": "lib/index.cjs",
      "limit": "800B"
    }
  ],
  "keywords": [
    "internationalization",
    "intl",
    "messageformat",
    "plural",
    "icu",
    "preact",
    "tiny",
    "i18n"
  ],
  "author": {
    "name": "Maksim Sharipov",
    "url": "https://github.com/pret-a-porter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibitcy/eo-locale.git"
  },
  "bugs": {
    "url": "https://github.com/ibitcy/eo-locale/issues"
  },
  "license": "ISC",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "preact": "^10.11.3",
    "rollup": "^3.9.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "size-limit": "^8.1.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "preact": "^10.1.1"
  },
  "dependencies": {
    "@eo-locale/core": "workspace:^"
  }
}
