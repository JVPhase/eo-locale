{
  "name": "@eo-locale/react",
  "version": "1.3.1",
  "description": "Easy way to internationalize your React application",
  "author": "Maksim Sharipov <m.v.sharipov@gmail.com>",
  "homepage": "https://eo-locale.netlify.app",
  "license": "ISC",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">= 12"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup --config ./rollup.config.js",
    "postbuild": "copyup \"src/**/*.flow\" lib && size-limit",
    "prepare": "flow && npm run test && npm run build",
    "test": "jest --detectOpenHandles --config ./config/jest.config.json --no-cache"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibitcy/eo-locale.git"
  },
  "bugs": {
    "url": "https://github.com/ibitcy/eo-locale/issues"
  },
  "keywords": [
    "internationalization",
    "intl",
    "messageformat",
    "plural",
    "icu",
    "react",
    "tiny",
    "i18n"
  ],
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "900B",
      "webpack": false
    }
  ],
  "dependencies": {
    "@eo-locale/core": "^1.3.1"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.0.2",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "gitHead": "3c5f02105fabe92451ce95efdbbbae7bc0ca74e4"
}
