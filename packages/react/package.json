{
  "name": "@eo-locale/react",
  "version": "1.4.0",
  "description": "Easy way to internationalize your React application",
  "author": "Maksim Sharipov <m.v.sharipov@gmail.com>",
  "homepage": "https://eo-locale.netlify.app",
  "license": "ISC",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">= 12"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup --config config/rollup.config.js",
    "postbuild": "flow && copyup \"src/**/*.flow\" lib && size-limit",
    "prepare": "npm run test && npm run build",
    "test": "jest --detectOpenHandles --config config/jest.config.json --no-cache"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibitcy/eo-locale.git"
  },
  "bugs": {
    "url": "https://github.com/ibitcy/eo-locale/issues"
  },
  "keywords": [
    "internationalization",
    "intl",
    "messageformat",
    "plural",
    "icu",
    "react",
    "tiny",
    "i18n"
  ],
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "900B",
      "webpack": false
    }
  ],
  "dependencies": {
    "@eo-locale/core": "^1.4.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.0.2",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "copyfiles": "^2.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "flow-bin": "^0.130.0",
    "jest": "^26.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.17.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "size-limit": "^4.5.0",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "gitHead": "85f16888e69bd8962e3e20ad92065a6ef5afcb22"
}
